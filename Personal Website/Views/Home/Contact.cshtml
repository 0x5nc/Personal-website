@{
    ViewBag.Title = "Contact";
}


<section id="contact">
    <div class="page-header">
        <h2 class="text-center">Contact me</h2>
    </div>
    <form class="form-horizontal col-md-9 col-md-offset-3">
        <div class="form-group">
            <label for="name" class="control-label col-md-2">Name</label>
            <div class="col-md-10">
                <input class="form-control " name="name" placeholder="Insert your name" type="text">
            </div>

        </div>
        <div class="form-group">
            <label for="email" class="control-label col-md-2">Email</label>
            <div class="col-md-10">
                <input class="form-control" placeholder="Insert your email" type="email">
            </div>
        </div>
        <div class="form-group">
            <label for="subject" class="control-label col-md-2">Subject</label>
            <div class="col-md-10">
                <input class="form-control" placeholder="Insert your subject" type="text">
            </div>
        </div>
        <div class="form-group">
            <label for="Comments" class="control-label col-md-2">Comments</label>
            <div class="col-md-10">
                <textarea class="form-control" rows="10"></textarea>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-10 col-md-offset-2">
                <input class="btn btn-success btn-block" type="submit">
            </div>
        </div>
        </div>
    </form>
</section>